{
  "spec_dir": "dist/tests",
  "spec_files": [
    "**/userSpec.js",
    "**/productSpec.js",
    "**/orderSpec.js"
],
"helpers": [
    "helpers/**/*.js"
],
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "all"
      }
    }
  ],
  "stopSpecOnExpectationFailure": false,
  "random": false
}